    <script type="text/javascript">
        var iteration = 1;
        var count = 0;
        var photos = new Array(7);
        var i = 0;
        var rand_1 = Math.ceil(Math.random(400) + 300)
        var rand_2 = Math.ceil(Math.random(400) + 300)

        var lower = 300;
        var upper = 1000;
        var url = "http://placekitten.com/"+(Math.floor(Math.random()*(upper-lower))+lower) + "/" + (Math.floor(Math.random()*(upper-lower))+lower);

        function loop() {
            for (i = 0; i < 1; i++) {
                $('#col1').append('<img src = url/><br/>');
                $('#col2').append('<img src="http://placekitten.com/g/200/300"/><br/>');
            }
            iteration++;
            count += 25;
            rand_1 = Math.ceil(Math.random(400) + 300)
            rand_2 = Math.ceil(Math.random(400) + 300)
        } 

        $(document).ready(function() {
            loop();
            $('#iterate').click(function() {
                loop();
            });
        });
    </script>
