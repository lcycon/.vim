import java.util.Scanner;
class MidtermGrade {

    public static void main (String[] args) {
        Scanner keyboard = new Scanner(System.in);
        System.out.println("To who it may concern: \n (press '1', please)");
        keyboard.nextLine();
        keyboard.nextLine();
        System.out.println("From my understanding the midterm was out of 20"
                + "points.");
    }
}
