import java.util.*;
class Test {

    public static void main(String[] args) {
        DoublyLinkedList12<String> list = new DoublyLinkedList12<String>();
        Iterator<String> iterator1 = list.iterator();
        for (int i = 0; i < 10; i++) {

            list.add(String.valueOf(i));
        }
        for (int j = 0; j < 10; j++) {
            iterator1.next();
        }
        list.removeBack();
        iterator1.remove();
        System.out.println(String.valueOf(list.size()));
    }
}
